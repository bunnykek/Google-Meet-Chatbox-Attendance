{
  "name": "Google Meet chatbox attendance",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Downloads the attendance from Google Meet chatbox.",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
},
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://meet.google.com//"
  ]
}